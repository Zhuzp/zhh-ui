#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhuzhengpin/Desktop/code/zhh-ui/node_modules/.pnpm/@umijs+fabric@4.0.1_postcss@8.4.38/node_modules/@umijs/fabric/node_modules:/Users/zhuzhengpin/Desktop/code/zhh-ui/node_modules/.pnpm/@umijs+fabric@4.0.1_postcss@8.4.38/node_modules/@umijs/node_modules:/Users/zhuzhengpin/Desktop/code/zhh-ui/node_modules/.pnpm/@umijs+fabric@4.0.1_postcss@8.4.38/node_modules:/Users/zhuzhengpin/Desktop/code/zhh-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhuzhengpin/Desktop/code/zhh-ui/node_modules/.pnpm/@umijs+fabric@4.0.1_postcss@8.4.38/node_modules/@umijs/fabric/node_modules:/Users/zhuzhengpin/Desktop/code/zhh-ui/node_modules/.pnpm/@umijs+fabric@4.0.1_postcss@8.4.38/node_modules/@umijs/node_modules:/Users/zhuzhengpin/Desktop/code/zhh-ui/node_modules/.pnpm/@umijs+fabric@4.0.1_postcss@8.4.38/node_modules:/Users/zhuzhengpin/Desktop/code/zhh-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
